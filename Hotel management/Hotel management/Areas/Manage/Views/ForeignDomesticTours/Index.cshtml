@model IEnumerable<Hotel_management.Models.ForeignDomesticTour>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Manage/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ForeignTourDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DomesticTourDescription)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ForeignTourDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DomesticTourDescription)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> 

            </td>
        </tr>
}
    </tbody>
</table>
